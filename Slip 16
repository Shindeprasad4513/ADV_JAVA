    String q2="select * from teacher where sub=?";
    pmt=con.prepareStatement(q2);
    pmt.setString(1,"java");
    ResultSet rs=pmt.executeQuery();
    while(rs.next())
    {
    	System.out.println("Teacher Deatails Who Teach Java\n"+rs.getInt("tno")+"\t"+rs.getString("tname")+"\t"+rs.getString("sub"));
    }
 }
}

